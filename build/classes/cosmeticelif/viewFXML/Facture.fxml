      <TextField fx:id="textPrix" layoutX="23.0" layoutY="232.0" />
      <Button fx:id="btnAdd" layoutX="114.0" layoutY="267.0" mnemonicParsing="false" onAction="#Ajouter" text="Ajouter" />
